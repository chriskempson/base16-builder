<%- black          = @base["00"]["rgb"] -%>
<%- blue           = @base["0D"]["rgb"] -%>
<%- green          = @base["0B"]["rgb"] -%>
<%- cyan           = @base["0C"]["rgb"] -%>
<%- red            = @base["08"]["rgb"] -%>
<%- magenta        = @base["0E"]["rgb"] -%>
<%- yellow         = @base["0A"]["rgb"] -%>
<%- white          = @base["05"]["rgb"] -%>
<%- bright_black   = @base["03"]["rgb"] -%>
<%- bright_blue    = @base["04"]["rgb"] -%>
<%- bright_green   = @base["01"]["rgb"] -%>
<%- bright_cyan    = @base["0F"]["rgb"] -%>
<%- bright_red     = @base["09"]["rgb"] -%>
<%- bright_magenta = @base["06"]["rgb"] -%>
<%- bright_yellow  = @base["02"]["rgb"] -%>
<%- bright_white   = @base["07"]["rgb"] -%>
<%- def print_color(color) -%>
<%-   %Q[r="#{color[0]}" g="#{color[1]}" b="#{color[2]}"] -%>
<%- end -%>
<?xml version="1.0"?>
<settings>
	<console>
		<colors>
			<color id="0" <%= print_color(black) %>/>
			<color id="1" <%= print_color(blue) %>/>
			<color id="2" <%= print_color(green) %>/>
			<color id="3" <%= print_color(cyan) %>/>
			<color id="4" <%= print_color(red) %>/>
			<color id="5" <%= print_color(magenta) %>/>
			<color id="6" <%= print_color(yellow) %>/>
			<color id="7" <%= print_color(white) %>/>
			<color id="8" <%= print_color(bright_black) %>/>
			<color id="9" <%= print_color(bright_blue) %>/>
			<color id="10" <%= print_color(bright_green) %>/>
			<color id="11" <%= print_color(bright_cyan) %>/>
			<color id="12" <%= print_color(bright_red) %>/>
			<color id="13" <%= print_color(bright_magenta) %>/>
			<color id="14" <%= print_color(bright_yellow) %>/>
			<color id="15" <%= print_color(bright_white) %>/>
		</colors>
	</console>
</settings>
