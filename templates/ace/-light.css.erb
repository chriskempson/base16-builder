.ace-tomorrow .ace_gutter {
  background: #<%= @base["07"]["hex"] %>;
  color: rgb(155,157,160)
}

.ace-tomorrow .ace_print-margin {
  width: 1px;
  background: #e8e8e8
}

.ace-tomorrow {
  background-color: #<%= @base["07"]["hex"] %>;
  color: #<%= @base["02"]["hex"] %>
}

.ace-tomorrow .ace_cursor {
  color: #<%= @base["02"]["hex"] %>
}

.ace-tomorrow .ace_marker-layer .ace_selection {
  background: #<%= @base["06"]["hex"] %>
}

.ace-tomorrow.ace_multiselect .ace_selection.ace_start {
  box-shadow: 0 0 3px 0px #<%= @base["07"]["hex"] %>;
  border-radius: 2px
}

.ace-tomorrow .ace_marker-layer .ace_step {
  background: rgb(198, 219, 174)
}

.ace-tomorrow .ace_marker-layer .ace_bracket {
  margin: -1px 0 0 -1px;
  border: 1px solid #<%= @base["06"]["hex"] %>
}

.ace-tomorrow .ace_marker-layer .ace_active-line {
  background: rgba(<%= @base["05"]["rgb"] %>, 0.33)
}

.ace-tomorrow .ace_gutter-active-line {
  background-color: rgba(<%= @base["05"]["rgb"] %>, 0.33)
}

.ace-tomorrow .ace_marker-layer .ace_selected-word {
  border: 1px solid #<%= @base["06"]["hex"] %>
}

.ace-tomorrow .ace_fold {
  background-color: #<%= @base["0D"]["hex"] %>;
  border-color: #<%= @base["02"]["hex"] %>
}

.ace-tomorrow .ace_keyword {
  color: #<%= @base["0E"]["hex"] %>
}

.ace-tomorrow .ace_keyword.ace_operator {
  color: #<%= @base["02"]["hex"] %>
}

.ace-tomorrow .ace_keyword.ace_other.ace_unit {
  color: #<%= @base["09"]["hex"] %>
}

.ace-tomorrow .ace_constant {
  color: #<%= @base["09"]["hex"] %>
}

.ace-tomorrow .ace_constant.ace_numeric {
  color: #<%= @base["09"]["hex"] %>
}

.ace-tomorrow .ace_constant.ace_character.ace_escape {
  color: #<%= @base["0C"]["hex"] %>
}

.ace-tomorrow .ace_support.ace_function {
  color: #<%= @base["0C"]["hex"] %>
}

.ace-tomorrow .ace_support.ace_class {
  color: #<%= @base["09"]["hex"] %>
}

.ace-tomorrow .ace_storage {
  color: #<%= @base["0E"]["hex"] %>
}

.ace-tomorrow .ace_invalid.ace_illegal {
  color: #<%= @base["07"]["hex"] %>;
  background-color: #<%= @base["08"]["hex"] %>
}

.ace-tomorrow .ace_invalid.ace_deprecated {
  color: #<%= @base["07"]["hex"] %>;
  background-color: #<%= @base["0F"]["hex"] %>
}

.ace-tomorrow .ace_string {
  color: #<%= @base["0B"]["hex"] %>
}

.ace-tomorrow .ace_string.ace_regexp {
  color: #<%= @base["0C"]["hex"] %>
}

.ace-tomorrow .ace_comment {
  color: #<%= @base["04"]["hex"] %>
}

.ace-tomorrow .ace_variable {
  color: #<%= @base["08"]["hex"] %>
}

.ace-tomorrow .ace_meta.ace_selector {
  color: #<%= @base["0E"]["hex"] %>
}

.ace-tomorrow .ace_entity.ace_other.ace_attribute-name {
  color: #<%= @base["09"]["hex"] %>
}

.ace-tomorrow .ace_entity.ace_name.ace_function {
  color: #<%= @base["0D"]["hex"] %>
}

.ace-tomorrow .ace_entity.ace_name.ace_tag {
  color: #<%= @base["08"]["hex"] %>
}

.ace-tomorrow .ace_markup.ace_list {
  color: #<%= @base["08"]["hex"] %>
}