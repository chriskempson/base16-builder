.highlight .hll { background-color: #<%= @base["02"]["hex"] %> }
.highlight  { background: #<%= @base["00"]["hex"] %>; color: #<%= @base["07"]["hex"] %> }
.highlight .c { color: #<%= @base["03"]["hex"] %> } /* Comment */
.highlight .err { color: #<%= @base["08"]["hex"] %> } /* Error */
.highlight .k { color: #<%= @base["0E"]["hex"] %> } /* Keyword */
.highlight .l { color: #<%= @base["09"]["hex"] %> } /* Literal */
.highlight .n { color: #<%= @base["07"]["hex"] %> } /* Name */
.highlight .o { color: #<%= @base["0C"]["hex"] %> } /* Operator */
.highlight .p { color: #<%= @base["07"]["hex"] %> } /* Punctuation */
.highlight .cm { color: #<%= @base["03"]["hex"] %> } /* Comment.Multiline */
.highlight .cp { color: #<%= @base["03"]["hex"] %> } /* Comment.Preproc */
.highlight .c1 { color: #<%= @base["03"]["hex"] %> } /* Comment.Single */
.highlight .cs { color: #<%= @base["03"]["hex"] %> } /* Comment.Special */
.highlight .gd { color: #<%= @base["08"]["hex"] %> } /* Generic.Deleted */
.highlight .ge { font-style: italic } /* Generic.Emph */
.highlight .gh { color: #<%= @base["07"]["hex"] %>; font-weight: bold } /* Generic.Heading */
.highlight .gi { color: #<%= @base["0B"]["hex"] %> } /* Generic.Inserted */
.highlight .gp { color: #<%= @base["03"]["hex"] %>; font-weight: bold } /* Generic.Prompt */
.highlight .gs { font-weight: bold } /* Generic.Strong */
.highlight .gu { color: #<%= @base["0C"]["hex"] %>; font-weight: bold } /* Generic.Subheading */
.highlight .kc { color: #<%= @base["0E"]["hex"] %> } /* Keyword.Constant */
.highlight .kd { color: #<%= @base["0E"]["hex"] %> } /* Keyword.Declaration */
.highlight .kn { color: #<%= @base["0C"]["hex"] %> } /* Keyword.Namespace */
.highlight .kp { color: #<%= @base["0E"]["hex"] %> } /* Keyword.Pseudo */
.highlight .kr { color: #<%= @base["0E"]["hex"] %> } /* Keyword.Reserved */
.highlight .kt { color: #<%= @base["0A"]["hex"] %> } /* Keyword.Type */
.highlight .ld { color: #<%= @base["0B"]["hex"] %> } /* Literal.Date */
.highlight .m { color: #<%= @base["09"]["hex"] %> } /* Literal.Number */
.highlight .s { color: #<%= @base["0B"]["hex"] %> } /* Literal.String */
.highlight .na { color: #<%= @base["0D"]["hex"] %> } /* Name.Attribute */
.highlight .nb { color: #<%= @base["07"]["hex"] %> } /* Name.Builtin */
.highlight .nc { color: #<%= @base["0A"]["hex"] %> } /* Name.Class */
.highlight .no { color: #<%= @base["08"]["hex"] %> } /* Name.Constant */
.highlight .nd { color: #<%= @base["0C"]["hex"] %> } /* Name.Decorator */
.highlight .ni { color: #<%= @base["07"]["hex"] %> } /* Name.Entity */
.highlight .ne { color: #<%= @base["08"]["hex"] %> } /* Name.Exception */
.highlight .nf { color: #<%= @base["0D"]["hex"] %> } /* Name.Function */
.highlight .nl { color: #<%= @base["07"]["hex"] %> } /* Name.Label */
.highlight .nn { color: #<%= @base["0A"]["hex"] %> } /* Name.Namespace */
.highlight .nx { color: #<%= @base["0D"]["hex"] %> } /* Name.Other */
.highlight .py { color: #<%= @base["07"]["hex"] %> } /* Name.Property */
.highlight .nt { color: #<%= @base["0C"]["hex"] %> } /* Name.Tag */
.highlight .nv { color: #<%= @base["08"]["hex"] %> } /* Name.Variable */
.highlight .ow { color: #<%= @base["0C"]["hex"] %> } /* Operator.Word */
.highlight .w { color: #<%= @base["07"]["hex"] %> } /* Text.Whitespace */
.highlight .mf { color: #<%= @base["09"]["hex"] %> } /* Literal.Number.Float */
.highlight .mh { color: #<%= @base["09"]["hex"] %> } /* Literal.Number.Hex */
.highlight .mi { color: #<%= @base["09"]["hex"] %> } /* Literal.Number.Integer */
.highlight .mo { color: #<%= @base["09"]["hex"] %> } /* Literal.Number.Oct */
.highlight .sb { color: #<%= @base["0B"]["hex"] %> } /* Literal.String.Backtick */
.highlight .sc { color: #<%= @base["07"]["hex"] %> } /* Literal.String.Char */
.highlight .sd { color: #<%= @base["03"]["hex"] %> } /* Literal.String.Doc */
.highlight .s2 { color: #<%= @base["0B"]["hex"] %> } /* Literal.String.Double */
.highlight .se { color: #<%= @base["09"]["hex"] %> } /* Literal.String.Escape */
.highlight .sh { color: #<%= @base["0B"]["hex"] %> } /* Literal.String.Heredoc */
.highlight .si { color: #<%= @base["09"]["hex"] %> } /* Literal.String.Interpol */
.highlight .sx { color: #<%= @base["0B"]["hex"] %> } /* Literal.String.Other */
.highlight .sr { color: #<%= @base["0B"]["hex"] %> } /* Literal.String.Regex */
.highlight .s1 { color: #<%= @base["0B"]["hex"] %> } /* Literal.String.Single */
.highlight .ss { color: #<%= @base["0B"]["hex"] %> } /* Literal.String.Symbol */
.highlight .bp { color: #<%= @base["07"]["hex"] %> } /* Name.Builtin.Pseudo */
.highlight .vc { color: #<%= @base["08"]["hex"] %> } /* Name.Variable.Class */
.highlight .vg { color: #<%= @base["08"]["hex"] %> } /* Name.Variable.Global */
.highlight .vi { color: #<%= @base["08"]["hex"] %> } /* Name.Variable.Instance */
.highlight .il { color: #<%= @base["09"]["hex"] %> } /* Literal.Number.Integer.Long */
